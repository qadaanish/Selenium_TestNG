<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Automation Suite" thread-count="8" parallels = "tests">
    
    <test name="Google Test">
        <parameter name="url" value="https://www.google.com"/>
       		 <parameter name="browser" value="chrome"/>
        
        <classes>
            <class name="testNGsession.GoogleTest"/>
        </classes>
    </test>
    
     <test name="OpenCart Test">
         <parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/register"/>
           <parameter name="browser" value="firefox"/>
        <classes>
            <class name="testNGsession.OpenCartTest"/>
        </classes>
    </test>
    
     <test name="Amazon Test">
         <parameter name="url" value="https://www.amazon.in/"/>
                 <parameter name="browser" value="edge"/>
        <classes>
            <class name="testNGsession.AmazonTest"/>
        </classes>
    </test>
    
    
</suite>
